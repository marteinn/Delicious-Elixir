<h2>Show link</h2>

<ul>

  <li>
    <strong>Title:</strong>
    <%= @link.title %>
  </li>

  <li>
    <strong>Url:</strong>
    <%= @link.url %>
  </li>

  <li>
    <strong>Description:</strong>
    <%= @link.description %>
  </li>

</ul>

<%= link "Edit", to: link_path(@conn, :edit, @link) %>
<%= link "Back", to: link_path(@conn, :index) %>
